   "source": [
    "def plot_color(table, file=None, directory='color'):\n",
    "    \n",
    "    if directory==None:\n",
    "        if file==None:\n",
    "            file = 'color.pdf'\n",
    "    else:\n",
    "        create_directory(directory)\n",
    "        if file==None:\n",
    "            file = os.path.join(directory, 'color.pdf')\n",
    "        else:\n",
    "            file = os.path.join(directory, file)\n",
    "\n",
    "    plt.clf()\n",
    "    plt.figure(figsize=(8,8))\n",
    "\n",
    "    plt.xlabel(r\"u-g\")\n",
    "    plt.ylabel(r\"r-i\")\n",
    "    plt.title(r\"Color-Color Plot\")\n",
    "\n",
    "    if type(table) == Table:\n",
    "        plt.scatter(table[color[0]]-table[colors[1]], table[colors[1]]-table[colors[2]], s=1, marker='.', c=plt_colors[0])\n",
    "        \n",
    "    elif any([type(table) == list, type(table) == tuple]):\n",
    "        for count, reg in enumerate(table):\n",
    "            if count == 0:\n",
    "                plt.scatter(reg[colors[0]]-reg[colors[1]], reg[colors[1]]-reg[colors[2]], s=1, marker='.', alpha=0.3, c=plt_colors[count])\n",
    "            else:\n",
    "                plt.scatter(reg[colors[0]]-reg[colors[1]], reg[colors[1]]-reg[colors[2]], s=1.5, marker='.', c=plt_colors[count])\n",
    "    else: return \"Error: Not recognized type.\"\n",
    "   \n",
    "\n",
    "    plt.xlim(-7,7)\n",
    "    plt.ylim(-7,7)\n",
    "    plt.savefig(file, format='pdf', dpi=72, clobber=True)\n",
    "    \n",
    "\"\"\"\n",
    "# Alternate Method.\n",
    "import seaborn as sns\n",
    "\n",
    "for count, reg in enumerate(regions):\n",
    "    sns.jointplot(x=reg['u']-reg['g'], y=reg['r']-reg['i'], kind='kde', xlim=(-5,5), ylim=(-5,5), color=plt_colors[count])\n",
    "    sns.jointplot(x=reg['u']-reg['g'], y=reg['r']-reg['i'], kind='scatter', xlim=(-5,5), ylim=(-5,5), s=1, color=plt_colors[count])\n",
    "\"\"\""
   ]
